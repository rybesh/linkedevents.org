        <foaf:name>Raphaël Troncy</foaf:name>
      </rdf:Description>
    </dcterm:contributor>
    <owl:versionInfo>2009-07-28</owl:versionInfo>
    <dcterm:contributor>
      <rdf:Description rdf:nodeID="ub1bL28C20">
        <foaf:name>Lynda Hardman</foaf:name>
        <foaf:homepage rdf:resource="http://www.cwi.nl/~lynda/"/>
      </rdf:Description>
    </dcterm:contributor>
    <skos:historyNote>
      <rdf:Description rdf:nodeID="ub1bL40C18">
